{
  "name": "yjtec/laravel-request-agent",
  "description": "laravel request agent macroable",
  "authors": [{
    "name": "kidkang",
    "email": "723616880@qq.com"
  }],
  "require": {
    "php": "^7.1",
    "illuminate/support": "^4.0|^5.0",
    "jenssegers/agent": "v2.6.0"
  },
  "require-dev": {
    "mockery/mockery": "~1.0",
    "phpunit/phpunit": "~6.0"
  },
  "autoload": {
    "psr-4": {
      "Yjtec\\Request\\Agent\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Yjtec\\Request\\Agent\\Tests\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": ["Yjtec\\Request\\Agent\\RequestServiceProvider"]
    }
  }
}
